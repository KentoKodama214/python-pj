CREATE TABLE WEO_GDP_norm(
  WEOCountryCode VARCHAR(10),
  ISO VARCHAR(10),
  WEOSubjectCode VARCHAR(20),
  Country VARCHAR(100),
  SubjectDescriptor VARCHAR(200),
  SubjectNotes VARCHAR(2000),
  Units VARCHAR(100),
  Scale VARCHAR(100),
  CountrySeriesSpecificNotes VARCHAR(4000),
  year INT,
  GDP DECIMAL(20,3)
);